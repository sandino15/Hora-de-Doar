<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="LOCATION_PERMISSION_REQUEST_CODE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gymfit2"
        tools:targetApi="31">
        <activity
            android:name=".NovoLucas.activityMenuu"
            android:exported="false" />
        <activity
            android:name=".NovoLucas.termoUso"
            android:exported="false" />
        <activity
            android:name=".NovoLucas.telaEmocentro"
            android:exported="false" />
        <activity
            android:name=".NovoLucas.pesquisaLocal"
            android:exported="false" />
        <activity
            android:name=".NovoLucas.pedirdoacao"
            android:exported="false" />
        <activity
            android:name=".NovoLucas.mensagemdoacao"
            android:exported="false" />
        <activity
            android:name=".Paginas.paginasMapas"
            android:exported="false"
             />
        <activity
            android:name=".activity.Menu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--
             TODO: Before you run your application, you need a Google Maps API key.

             To get one, follow the directions here:

                https://developers.google.com/maps/documentation/android-sdk/get-api-key

             Once you have your API key (it starts with "AIza"), define a new property in your
             project's local.properties file (e.g. MAPS_API_KEY=Aiza...), and replace the
             "YOUR_API_KEY" string in this file with "${MAPS_API_KEY}".
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_API_KEY" />

        <activity
            android:name=".Maps.MapsIhene"
            android:exported="false"
            android:label="@string/title_activity_maps_ihene" />
        <activity
            android:name=".Hospitais.RedesHospital"
            android:exported="false" />

        <provider
            android:name="com.google.android.gms.ads.MobileAdsInitProvider"
            android:authorities="${applicationId}.com.google.android.gms.ads.MobileAdsInitProvider"
            android:exported="false"
            tools:node="remove" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="seu_app_id_do_admob" />

        <activity
            android:name=".Maps.MapsActivity"
            android:exported="false"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".Paginas.Peito"
            android:exported="false" />
        <activity
            android:name=".Paginas.TreinoInferiores"
            android:exported="false" />
        <activity
            android:name=".Paginas.TreinosSuperiores"
            android:exported="false" />
        <activity
            android:name=".Paginas.PaginaPrincipal"
            android:exported="true" />
        <activity
            android:name=".activity.Exercicios"
            android:exported="false" />
        <activity
            android:name=".activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".CriarConta.CadastroActivity"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:label="Minha MainActivity" />
    </application>

</manifest>